#: pyhera.py:40
msgid "Heuristic Reasoning Agent – a service and a daemon for simple AI tasks."
msgstr "Heuristic Reasoning Agent – en tjeneste og en daemon for enkle AI oppgaver."

#: pyhera.py:41
msgid "For further help, check the documentation."
msgstr "For videre hjelp, sjekk dokumentasjonen."

#: pyhera.py:44
msgid "calculate and allocate, but do not load"
msgstr "kalkuler og alloker, men last ikke inn"

#: pyhera.py:48
msgid "turn on increasing verbose mode"
msgstr "slå på økende ordrik modus"

#: pyhera.py:51
msgid "name for the specific agent"
msgstr "navn for typen agent"

#: pyhera.py:54
msgid "identifier for the specific agent"
msgstr "identifikator for typen agent"

#: pyhera.py:57
msgid "continue without asking"
msgstr "fortsett uten spørsmål"

#: pyhera.py:59
msgid "subcommands"
msgstr "underkommandoer"

#: pyhera.py:60
msgid "valid subcommands"
msgstr "gyldige underkommandoer"

#: pyhera.py:61
msgid "help for subcommands"
msgstr "hjelp for underkommandoer"

#: pyhera/build.py:55
msgid "build an agent"
msgstr "bygg en agent"

#: pyhera/build.py:56
msgid "Build the agent, but do not load or run it"
msgstr "Bygg agenten, men last den ikke inn eller kjør den"

#: pyhera/build.py:60
msgid "type name for the agent"
msgstr "navn på agentens type"

#: pyhera/build.py:64
msgid "layout name for the agent"
msgstr "navn på agentens utlegg"

#: pyhera/destroy.py:23
msgid "destroy an agent"
msgstr "ødelegg en agent"

#: pyhera/destroy.py:24
msgid "Destroy the agent, unless it is running"
msgstr "Ødelegg agenten, hvis den ikke kjører"

#: pyhera/up.py:23
msgid "load and start an agent"
msgstr "last inn og start en agent"

#: pyhera/up.py:24
msgid "Load and start the agent"
msgstr "Last inn og kjør agenten"

#: pyhera/up.py:27
msgid "run as daemon if possible (abort on command mode)"
msgstr "kjør som demon hvis mulig (avbryt ved kommandomodus)"

#: pyhera/up.py:30
msgid "check if the agent loads, and then abort"
msgstr "sjekk om agenten laster, og avbryt deretter"

#: pyhera/up.py:33
msgid "open command mode"
msgstr "åpne kommandomodus"

#: pyhera/up.py:37
msgid "one or N single steps in command mode"
msgstr "én eller N enkle steg i kommandomodus"

#: pyhera/up.py:40
msgid "index for device to populate"
msgstr "indeks for enheten som skal befolkes"

#: pyhera/down.py:23
msgid "stop and unload an agent"
msgstr "stopp og last ut en agent"

#: pyhera/down.py:24
msgid "Stop and unload the agent"
msgstr "Stopp og last ut agenten"

#: pyhera/agents.py:23
msgid "list all available agents"
msgstr "list alle tilgjengelige agenter"

#: pyhera/agents.py:24
msgid "List all available agents"
msgstr "List alle tilgjengelige agenter"

#: pyhera/agents.py:27
msgid "list only this agent name"
msgstr "list bare denne navngitte agenten"

#: pyhera/devices.py:23
msgid "list all available devices"
msgstr "list alle tilgjengelige enheter"

#: pyhera/devices.py:24
msgid "List all available devices"
msgstr "List alle tilgjengelige enheter"

#: pyhera/devices.py:27
msgid "list only this device name"
msgstr "list bare denne navngitte enheten"

#: pyhera/layouts.py:29
